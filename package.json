{
  "name": "labofolio-api",
  "version": "1.0.0",
  "description": "labofolio api server",
  "author": "foway0",
  "license": "ISC",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "DEBUG_COLORS=true DEBUG=labofolio:* nodemon -e ts,yaml -w src -x 'ts-node ./src/index.ts'",
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "build": "run-s clean tsc build:yaml",
    "build:yaml": "cp -r src/api_specs dist",
    "lint": "eslint --ext .ts ./src",
    "lint:fix": "eslint --fix --ext .ts ./src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@jsdevtools/ono": "^7.1.2",
    "express": "^4.17.1",
    "express-openapi-validator": "^3.16.6",
    "ioredis": "^4.17.3",
    "mysql2": "^2.1.0",
    "sequelize": "^6.3.3",
    "sprintf-js": "^1.1.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.32",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.7",
    "@types/ioredis": "^4.17.2",
    "@types/multer": "^1.4.3",
    "@types/node": "^14.0.23",
    "@types/sprintf-js": "^1.1.2",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "debug": "^4.1.1",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foway0/labofolio-api.git"
  },
  "bugs": {
    "url": "https://github.com/foway0/labofolio-api/issues"
  },
  "homepage": "https://github.com/foway0/labofolio-api#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
