{
  "name": "labofolio-api",
  "version": "1.0.0",
  "description": "labofolio api server",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "DEBUG_COLORS=true DEBUG=labofolio:* nodemon -e ts,yaml -w src -x 'ts-node ./src/index.ts'",
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "build": "run-s clean tsc build:yaml",
    "build:yaml": "cp -r src/api_specs dist",
    "lint": "eslint --ext .ts ./src",
    "lint:fix": "eslint --fix --ext .ts ./src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foway0/labofolio-api.git"
  },
  "author": "foway0",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/foway0/labofolio-api/issues"
  },
  "homepage": "https://github.com/foway0/labofolio-api#readme",
  "dependencies": {
    "express": "^4.17.1",
    "express-openapi-validator": "^3.9.2",
    "ioredis": "^4.16.0",
    "mysql2": "^2.1.0",
    "sequelize": "^5.21.5"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/ioredis": "^4.14.8",
    "@types/multer": "^1.4.2",
    "@types/node": "^13.7.6",
    "@types/validator": "^12.0.1",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "debug": "^4.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2"
  }
}
